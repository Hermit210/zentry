# Production Environment Configuration for Zentry Cloud

# Environment
ENVIRONMENT=production
DEBUG=false

# Supabase Configuration (Production)
SUPABASE_URL=https://your-production-project.supabase.co
SUPABASE_ANON_KEY=your-production-anon-key
SUPABASE_SERVICE_KEY=your-production-service-key

# JWT Configuration (MUST be secure in production)
SECRET_KEY=your-super-secure-production-jwt-key-minimum-32-characters-long
ALGORITHM=HS256
ACCESS_TOKEN_EXPIRE_MINUTES=30

# API Configuration
API_HOST=0.0.0.0
API_PORT=8000
API_RELOAD=false

# CORS Origins (Production domains only)
CORS_ORIGINS=https://yourdomain.com,https://www.yourdomain.com

# Database Configuration (if using direct PostgreSQL)
DATABASE_URL=postgresql://username:password@production-db-host:5432/zentry_production

# Production Settings
USE_IN_MEMORY_DB=false
ENABLE_DEMO_MODE=false